#!/usr/bin/env ruby
# frozen_string_literal: true

require 'bundler/setup'
require 'aoc2025'

day = ARGV[0].to_i
part = ARGV[1].to_i
test_input = ARGV[2] == 'true'
solution = Aoc2025.solve(day, part, test_input)

puts "Day #{day}, part #{part}, test input #{test_input}"
puts "Solution: #{solution}"
